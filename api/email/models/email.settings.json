{
  "kind": "collectionType",
  "collectionName": "emails",
  "info": {
    "name": "email"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "emails"
    },
    "email": {
      "type": "string",
      "maxLength": 255,
      "required": true,
      "unique": false
    },
    "is_verified": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
